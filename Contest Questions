//Weekly Contest Leetcode 
https://leetcode.com/contest/weekly-contest-470/problems/compute-alternating-sum/
//Q1. Compute Alternating Sum
class Solution {
    public int alternatingSum(int[] nums) {
        int sum=0;
        for(int i=0;i<nums.length;i++){
            if(i%2==0){
                sum+=nums[i];
            }else{
            sum-=nums[i];
            }
        }
        return sum;
    }
}
//Q2.Longest Subsequence With Non-Zero Bitwise XOR
https://leetcode.com/contest/weekly-contest-470/problems/longest-subsequence-with-non-zero-bitwise-xor/description/
class Solution {
    public int longestSubsequence(int[] nums) {
        int maxCnt=0;
        for(int i=0;i<nums.length;i++){
                maxCnt^=nums[i];
        }
        boolean allZero=true;
        for(int num:nums){
            if(num!=0){
            allZero=false;
            break;
            }
        }
            if(maxCnt!=0){
                return nums.length;
            }else if(!allZero){
              return  nums.length-1;
            }else return 0;
    }
}
//Q1. Equal Score Substrings
https://leetcode.com/contest/biweekly-contest-167/problems/equal-score-substrings/description/
class Solution {
    public boolean scoreBalance(String s) {
        double sum=0;
        List<Integer>list=new ArrayList<>();
        for(int i=0;i<s.length();i++){
            int num=s.charAt(i)-'a'+1;
            sum+=num;
            list.add(num);
        }
            int CurrSum=0;
            for(int i=0;i<list.size();i++){
                CurrSum+=list.get(i);
                if(CurrSum==sum/2){
                    return true;
                }
            }
        return false;
        }
}
//Longest Fibonacci Subarray
https://leetcode.com/contest/biweekly-contest-167/problems/longest-fibonacci-subarray/description/
class Solution {
    public int longestSubarray(int[] nums) {
        int Mcnt=2;
        int i=0;int j=i+1;int k=i+2;
        if(nums.length==1 || nums.length==2){
            return Mcnt;
        }
            int cnt=2;
         while(k<nums.length){
             if(nums[i]+nums[j]==nums[k]){
                 cnt++;
                 if(Mcnt<cnt){
                 Mcnt=cnt;
             }
             }else{
                 cnt=2;
             }
             i++;j++;k++;
        }
        return Mcnt;
    }
}
//Maximum Substrings With Distinct Start©leetcode
https://leetcode.com/contest/weekly-contest-478/problems/maximum-substrings-with-distinct-start/
class Solution {
    public int maxDistinct(String s) {
        boolean[]vis = new boolean[26];
        int cnt = 0;
        
        for(char c:s.toCharArray()) {
            if(!vis[c-'a']) {
                vis[c-'a'] = true;
                cnt++;
            }
        }
        return cnt;
    }
}
//Q1. Complete Prime Number©leetcode
https://leetcode.com/contest/biweekly-contest-171/problems/complete-prime-number/description/
class Solution {
    private boolean isPrime(int num) {
        if (num<2) return false;
        if (num%2==0) return num==2; 
        for (int c=3;c*c<=num;c+= 2) {
            if (num%c== 0) return false; 
        }
        return true;
    }
    public boolean completePrime(int num) {
         String str=String.valueOf(num);
        int n=str.length();
        if (n==1) return isPrime(num);
        for (int i=1;i<=n;i++) {
            int pF=Integer.parseInt(str.substring(0, i));
            int sF=Integer.parseInt(str.substring(n-i));
            if (!isPrime(pF) || !isPrime(sF)) return false;
        }
        return true;
    }
    
}
//Q4. Minimum Inversion Count in Subarrays of Fixed Length©leetcode
https://leetcode.com/contest/biweekly-contest-171/problems/minimum-inversion-count-in-subarrays-of-fixed-length/description/
class Solution {
    private int countInv(int[] nums,int start,int end){
        int cnt=0;
        for(int i=start;i<end;i++){
            for(int j=i+1;j<=end;j++){
                if(nums[i]>nums[j]){
                    cnt++;
                }
            }
        }
        return cnt;
    }
    public long minInversionCount(int[] nums, int k) {
        int n=nums.length;
        int MinInv=Integer.MAX_VALUE;
        for(int i=0;i<=n-k;i++){
            int temp=countInv(nums,i,i+k-1);
            MinInv=Math.min(MinInv,temp);
        }
        return MinInv;
    }
}
//3606. Coupon Code Validator
https://leetcode.com/problems/coupon-code-validator/description/?envType=daily-question&envId=2025-12-13
class Solution {
    public List<String> validateCoupons(String[] code, String[] businessLine, boolean[] isActive) { 
        int n=code.length;
        List<String> list=new ArrayList<>();
        for(int i=0;i<n;i++){
            boolean a=isActive[i];
            boolean c=code[i].matches("[A-Za-z0-9_]+");
            boolean valid=businessLine[i].equals("electronics")|| 
                businessLine[i].equals("grocery") ||
                businessLine[i].equals("pharmacy") ||
                businessLine[i].equals("restaurant");
            if(a && c && valid) {
                list.add(code[i]);
            }
        }
        Collections.sort(list);
        return list;
    }
}
