//Length of Linked List
https://www.geeksforgeeks.org/problems/count-nodes-of-linked-list/1
class Solution {
    public int getCount(Node head) {
    int cnt=0;
    Node temp=head;
    while(temp!=null){
        cnt++;
        temp=temp.next;
    }
        return cnt;
    }
}

//M-2 via Recursion 
class Solution {
    public int getCount(Node head) {
     if(head==null){
         return 0;
     }
     return 1+getCount(head.next);
    }
}
